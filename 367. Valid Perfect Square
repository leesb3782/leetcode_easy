// 367. Valid Perfect Square
// https://leetcode.com/problems/valid-perfect-square
bool isPerfectSquare(int num) {
    int i;

    for(i=1;i<=num/i;i++) {
        if(num%i>0)
            continue;
        else {
            if(i == num/i)
                break;
            else
                continue;
        }
    }
    return i == num/i;
}
